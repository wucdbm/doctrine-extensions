{
    "name": "wucdbm/doctrine-extensions",
    "type": "library",
    "description": "A custom set of Doctrine Extensions to ease development",
    "keywords": ["doctrine", "doctrine extensions", "bitmask", "utc date", "utc date time"],
    "authors": [
        {
            "name": "Martin Kirilov",
            "email": "wucdbm@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "doctrine/dbal": "^2.8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.8",
        "wucdbm/php-cs-fixers": "^0.3.0",
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4": { "Wucdbm\\DoctrineExtensions\\": "src" }
    },
    "autoload-dev": {
        "psr-4": {
            "Wucdbm\\DoctrineExtensions\\Tests\\": "tests/"
        }
    },
    "config": {
        "bin-dir": "bin",
        "platform": {
            "php": "7.1.3"
        }
    },
    "license": "MIT"
}